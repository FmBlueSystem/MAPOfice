# Example Environment Configuration for HAMMS v3.0 + OpenAI Integration
# Copy this file to .env and fill in your actual values

# OpenAI Configuration - REQUIRED
OPENAI_API_KEY=sk-your-openai-api-key-here
OPENAI_MODEL=gpt-4
OPENAI_MAX_TOKENS=1000
OPENAI_TEMPERATURE=0.1

# HAMMS v3.0 Configuration
HAMMS_CACHE_SIZE=1000
HAMMS_VECTOR_DIMENSIONS=12

# AI Integration Configuration
AI_BATCH_SIZE=5
AI_RETRY_ATTEMPTS=3
AI_TIMEOUT_SECONDS=30
AI_RATE_LIMIT_RPM=60

# Database Configuration  
DATABASE_PATH=data/music.db
DATABASE_TIMEOUT_MS=5000

# UI Configuration
UI_HAMMS_RADAR_ENABLED=true
UI_AI_PANEL_ENABLED=true

# Development Configuration
DEBUG_MODE=false
LOG_LEVEL=INFO
TELEMETRY_ENABLED=false